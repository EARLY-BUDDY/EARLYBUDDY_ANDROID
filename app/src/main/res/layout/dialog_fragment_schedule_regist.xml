<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/img_popup_plan"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="26dp"
                android:layout_marginTop="127dp"
                android:layout_marginRight="26dp"
                android:fontFamily="@font/noto_sans_kr_regular"
                android:text="일정 등록이 완료되었어요!"
                android:textColor="@color/black"
                android:textSize="17dp" />

            <TextView
                android:id="@+id/dialog_fragment_sche_pop_up_tv_check"
                android:layout_width="200dp"
                android:layout_height="36dp"
                android:layout_gravity="center"
                android:layout_marginTop="35dp"
                android:background="@drawable/act_place_round_rect_blue_full"
                android:gravity="center"
                android:fontFamily="@font/noto_sans_kr_medium"
                android:text="확인하러 가기"
                android:textColor="@color/white"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/dialog_fragment_sche_pop_up_tv_home"
                android:layout_width="200dp"
                android:layout_height="36dp"
                android:layout_gravity="center"
                android:layout_marginTop="12dp"
                android:background="@drawable/border_25_3092ff"
                android:fontFamily="@font/noto_sans_kr_medium"
                android:gravity="center"
                android:text="홈으로 가기"
                android:textColor="@color/main_color"
                android:textSize="15dp" />
        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>